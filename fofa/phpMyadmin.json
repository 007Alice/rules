{
    "name": "phpMyadmin",
    "author": "fofa",
    "version": "0.1.0",
    "matches": [
        {
            "search": "headers",
            "text": "Set-Cookie: phpMyAdmin="
        },
        {
            "search": "body",
            "regexp": "<title>.*phpMyAdmin .*<\\/title>"
        },
        {
            "search": "body",
            "text": "pma_password"
        }
    ]
}